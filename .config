CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_ROOTFS_EXT4FS=y  # 生成 ext4 镜像（适合U盘）
CONFIG_GRUB_IMAGES=y           # 启用 EFI 引导（J4125 通常需要）
CONFIG_GRUB_EFI_IMAGES=y

# 添加 OpenClash 依赖
CONFIG_PACKAGE_luci-app-openclash=y
CONFIG_PACKAGE_luci-i18n-openclash-zh-cn=y  # 中文语言包
CONFIG_PACKAGE_dnsmasq-full=y  # 替换 dnsmasq（OpenClash 需要）
CONFIG_PACKAGE_dnsmasq_dhcpv6=n
